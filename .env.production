# Database - Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Redis (Optional - コメントアウトでキャッシュなしでも動作)
# REDIS_URL=redis://localhost:6379

# JWT Secrets (本番環境では必ず変更！)
JWT_SECRET=dev-jwt-secret-key-change-in-production-12345678901234567890
JWT_REFRESH_SECRET=dev-jwt-refresh-secret-key-change-in-production-12345678901234567890

# Google Maps API (オプション)
GOOGLE_MAPS_API_KEY=

# V2.0 Automation Settings
GOOGLE_ELEVATION_API_ENABLED=false
GOOGLE_ELEVATION_API_TIMEOUT=10000
GOOGLE_ELEVATION_API_RETRY_COUNT=3
GOOGLE_ELEVATION_API_RETRY_DELAY=1000

# V2.0 Scoring Settings
SCORING_FORMULA_VERSION=v2.0.1
SCORING_WEIGHTS_GRID=0.35
SCORING_WEIGHTS_GEO=0.25
SCORING_WEIGHTS_REGULATORY=0.20
SCORING_WEIGHTS_ACCESS=0.10
SCORING_WEIGHTS_ECONOMICS=0.10

# V2.0 Batch Processing
BATCH_PROCESSING_MAX_CONCURRENT=5
BATCH_PROCESSING_TIMEOUT=300000

# Server
PORT=4000
NODE_ENV=development

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

# Monitoring
ENABLE_METRICS=true
METRICS_PORT=9090

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
LOG_DIR=./logs
