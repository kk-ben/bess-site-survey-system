# Database
DATABASE_URL=postgresql://bess_user:bess_password@localhost:5432/bess_survey

# Supabase (v2.0)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Redis (Caching - Optional)
REDIS_URL=redis://:redis_password@localhost:6379

# JWT Secrets (Change in production!)
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_REFRESH_SECRET=your-super-secret-refresh-key-change-in-production

# Google Maps API
GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# V2.0 Automation Settings
GOOGLE_ELEVATION_API_ENABLED=true
GOOGLE_ELEVATION_API_TIMEOUT=10000
GOOGLE_ELEVATION_API_RETRY_COUNT=3
GOOGLE_ELEVATION_API_RETRY_DELAY=1000

# V2.0 Scoring Settings
SCORING_FORMULA_VERSION=v2.0.1
SCORING_WEIGHTS_GRID=0.35
SCORING_WEIGHTS_GEO=0.25
SCORING_WEIGHTS_REGULATORY=0.20
SCORING_WEIGHTS_ACCESS=0.10
SCORING_WEIGHTS_ECONOMICS=0.10

# V2.0 Batch Processing
BATCH_PROCESSING_MAX_CONCURRENT=5
BATCH_PROCESSING_TIMEOUT=300000

# Email Configuration
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USER=your-email@gmail.com
EMAIL_PASS=your-app-password

# External APIs
OSM_OVERPASS_URL=https://overpass-api.de/api/interpreter
CHUDEN_CSV_URL=https://example.com/chuden-data.csv

# File Upload
MAX_FILE_SIZE=10485760
UPLOAD_DIR=./uploads

# Logging
LOG_LEVEL=info
LOG_DIR=./logs

# Server
PORT=4000
NODE_ENV=development

# CORS - Comma-separated list of allowed origins
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173,https://bess-site-survey-system.vercel.app,https://ps-system.jp

# Monitoring
ENABLE_METRICS=true
METRICS_PORT=9090

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
